<% if(typeof errors != 'undefined') {%> 
	<% errors.forEach(function(error){ %>
		<div key=1 class="alert alert-warning alert-dismissible fade show" role="alert">
				<%= error.mg %>
			<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
		</div>
	<%}); %>
<% } %>

<% if(success_msg != ''){ %> 
	<div key=2 class="alert alert-success alert-dismissible fade show" role="alert">
		<%= success_msg %>
	<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% }  %>

<% if(error_msg != ''){ %> 
	<div key=3 class="alert alert-warning alert-dismissible fade show" role="alert">
		<%= error_msg %>
	<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% }  %>

<% if(error != ''){ %> 
	<div key=4 class="alert alert-warning alert-dismissible fade show" role="alert">
		<%= error %>
	<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% }  %>